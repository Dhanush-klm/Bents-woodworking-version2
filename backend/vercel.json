{
  "version": 2,
  "builds": [
    {
      "src": "index.cjs",
      "use": "@vercel/node",
      "config": {
        "maxDuration": 300
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.cjs",
      "headers": {
        "x-timeout": "300000"
      },
      "timeoutMs": 300000
    }
  ]
}
