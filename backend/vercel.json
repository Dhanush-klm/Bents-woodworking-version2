{
  "version": 2,
  "builds": [
    {
      "src": "index.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "index.cjs"
    }
  ]
}
{
  "functions": {
    "index.cjs": {
      "maxDuration": 300 // This function can run for a maximum of 30 seconds
    }
  }
}
